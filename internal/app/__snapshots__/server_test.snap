    <style>
      html, body {
      height: 100%;
      margin: 0;
      }

      body {
      display: flex;
      flex-direction: column;
      align-items: center;
      justify-content: center;
      }

      main {
      flex: 1;
      }

      section {
      margin-top: 100px;
      }

      .lock-btn {
      display: inline-block;
      border: solid black 1px;
      border-radius: 4px;
      padding: 4px 10px;
      margin-bottom: 20px;
      cursor: pointer;
      }

      .task {
      background-color: #9fb4a7;
      border-radius: 3px;
      padding: 6px 10px;
      margin: 5px 0;
      color: #144418;
      position: relative;
      }

      .task.htmx-swapping {
      opacity: 0;
      transition: opacity 500ms ease-out;
      }

      .task-checkbox {
      margin-right: 18px;
      }

      .task-name.editable {
      cursor: pointer;
      }


      .task-name.done {
      text-decoration: line-through;
      }

      .task-name.editable:hover {
      text-decoration: underline;
      }

      .new-task-btn {
      border-radius: 3px;
      background-color: #dbe7df;
      color: #144418;
      font-style: italic;
      border: 1px dashed #144418;
      padding: 10px 20px;
      cursor: pointer;
      text-align: center;
      }

      .handle {
      margin-right: 10px;
      display: inline;
      position: relative;
      top: -2px;
      cursor: pointer;
      }

      .remove-task-btn {
      position: absolute;
      right: 8px;
      top: 6px;
      font-size: 15px;
      font-weight: 400;
      color: #fff;
      padding: 0;
      border: none;
      cursor: pointer;
      }
    </style>
    <script type="text/javascript">
      htmx.onLoad(function (content) {
      var taskList = content.querySelector('.active-tasks');
      new Sortable(taskList, {
      handle: '.handle',
      animation: 150,
      });
      })
    </script>
